{
  "name": "nblas",
  "version": "2.1.11",
  "description": "C++ bindings for all single- and double-precision CBLAS (Basic Linear Algebra Subprograms) routines.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "docs": "typedoc --out docs --exclude \"**/*+(.spec).ts\" src && touch docs/.nojekyll",
    "build": "node-gyp rebuild -j max && rm -rf dist && tsc",
    "install": "npm run build"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "dist/**",
      "docs/**",
      "coverage/**",
      "src/*.spec.ts",
      "src/index.cc"
    ],
    "reporter": [
      "lcov"
    ],
    "all": true
  },
  "repository": {
    "git": "https://github.com/mateogianolio/nblas"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.4",
    "typescript": "^5.0.4"
  },
  "author": "Mateo Gianolio",
  "license": "MIT",
  "gypfile": true
}
